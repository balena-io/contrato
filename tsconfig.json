{
	"compilerOptions": {
		"module": "commonjs",
		"esModuleInterop": true,
		"outDir": "build",
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"removeComments": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2015",
		"declaration": true,
		"skipLibCheck": true,
		"noImplicitAny": false,
		"resolveJsonModule": true
  }
}
